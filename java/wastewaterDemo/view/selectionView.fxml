<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="50.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wastewaterTeam7.view.selectionViewController">
   <left>
      <HBox prefHeight="400.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0">
               <children>
                  <Button fx:id="btnPlantInfo" mnemonicParsing="false" onAction="#goPlantInfo" prefHeight="50.0" prefWidth="100.0" text="Plant Info" />
                  <Button fx:id="btnContract" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#goContract" prefHeight="50.0" prefWidth="100.0" text="Contracts" />
                  <Button fx:id="btnPLCEquipment" layoutX="10.0" layoutY="35.0" mnemonicParsing="false" onAction="#goEquipment" prefHeight="50.0" prefWidth="100.0" text="Equipment" />
                  <Button fx:id="btnPLCpio" layoutX="10.0" layoutY="60.0" mnemonicParsing="false" onAction="#goPIO" prefHeight="50.0" prefWidth="100.0" text="PLC Points" />
                  <Button fx:id="btnUsers" layoutX="10.0" layoutY="85.0" mnemonicParsing="false" onAction="#goUser" prefHeight="50.0" prefWidth="100.0" text="Users" />
               </children>
            </VBox>
         </children>
      </HBox>
   </left>
</BorderPane>
